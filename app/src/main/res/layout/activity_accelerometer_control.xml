<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".accelerometerControl">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="12dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="-50dp"
        android:text="m"
        app:layout_constraintEnd_toStartOf="@+id/txt_uSonic_distanceID3"
        app:layout_constraintTop_toBottomOf="@+id/swi_uSonicID3" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="431dp"
        android:layout_height="155dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        app:layout_constraintBottom_toTopOf="@+id/directionBarID"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tableau_de_boord" />

    <ImageView
        android:id="@+id/swID3"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="149dp"
        android:rotation="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/abc_def" />

    <TextView
        android:id="@+id/angleTextID3"
        android:layout_width="189dp"
        android:layout_height="36dp"
        android:text="Angle : 0Â°"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Switch
        android:id="@+id/swi_uSonicID3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="16dp"
        android:text="Buzzer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_uSonic_distanceID3"
        android:layout_width="18dp"
        android:layout_height="19dp"
        android:text="0"
        tools:layout_editor_absoluteX="785dp"
        tools:layout_editor_absoluteY="39dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="262dp"
        android:layout_height="178dp"
        android:layout_marginTop="72dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/machine"
        tools:layout_editor_absoluteX="512dp" />

    <TextView
        android:id="@+id/powerLevelTextID3"
        android:layout_width="160dp"
        android:layout_height="37dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:text="Speed: 0"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/batterylevel_numID3"
        android:layout_width="98dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:text="100%"
        app:layout_constraintBottom_toTopOf="@+id/batteryLevelID3"
        app:layout_constraintStart_toEndOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Battery level:"
        app:layout_constraintBottom_toTopOf="@+id/batteryLevelID3"
        tools:layout_editor_absoluteX="492dp" />

    <ProgressBar
        android:id="@+id/batteryLevelID3"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="144dp"
        android:layout_height="43dp"
        android:layout_marginStart="492dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/powerLevelTextID2"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/parameterButtonID3"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginStart="328dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="1dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/batteryLevelID"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_settings" />

    <ImageButton
        android:id="@+id/clignot_droit_buttonID3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="1dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitCenter"
        android:tint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/backToFuture_buttonID3"
        app:srcCompat="@drawable/clignot" />

    <ImageButton
        android:id="@+id/backToFuture_buttonID3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="1dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitCenter"
        android:tint="#00000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/feuxdepanne_buttonID3"
        app:srcCompat="@drawable/logo_app" />

    <ImageButton
        android:id="@+id/feuxdepanne_buttonID3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="1dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitCenter"
        android:tint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/positionLightButtonID3"
        app:srcCompat="@drawable/feux_de_panne" />

    <ImageButton
        android:id="@+id/positionLightButtonID3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="1dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitCenter"
        android:tint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/clignot_gauche_buttonID3"
        app:srcCompat="@drawable/feux_de_position" />

    <ImageButton
        android:id="@+id/clignot_gauche_buttonID3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="1dp"
        android:backgroundTint="#00FFFFFF"
        android:rotation="180"
        android:scaleType="fitCenter"
        android:tint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/clignot" />
</androidx.constraintlayout.widget.ConstraintLayout>